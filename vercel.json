{
    "version": 2,
    "builds": [
      { "src": "/src/index.js", "use": "@vercel/static" }
    ],
    "routes": [
      { "src": "/old-path", "dest": "/new-path", "status": 301 },
      { "src": "/api/hello", "dest": "api/hello.js" }
    ],
    "headers": [
      {
        "source": "/(.*)",
        "headers": [
          {
            "key": "X-Custom-Header",
            "value": "MyHeaderValue"
          }
        ]
      }
    ],
    "redirects": [
      {
        "source": "/google",
        "destination": "https://google.com",
        "permanent": true
      }
    ]
  }
  